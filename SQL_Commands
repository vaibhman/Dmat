#CREATE DATABASE dmatdb;

#Create Tables
CREATE TABLE userAccounts(
	accountNo INT NOT NULL PRIMARY KEY,
	userName VARCHAR(50),
	accountBalance FLOAT(11,2),
	password VARCHAR(50)
);

